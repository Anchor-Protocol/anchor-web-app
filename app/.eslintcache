   ) {\n      Height\n      Result\n    }\n  }\n`;\n\nexport default function useContractBalance(\n  contractAddress: string,\n): [boolean, ApolloError | undefined, ContractBalanceResponse, MantleRefetch] {\n  const addressProvider = useAddressProvider();\n  const wallet = useWallet();\n  const { loading, error, data, refetch } = useQuery<{\n    contractBalance: MantleContractResponse;\n  }>(queryAnchorBalance, {\n    variables: {\n      contractAddress: contractAddress,\n      queryMsg: JSON.stringify({ balance: { address: wallet.address } }),\n    },\n  });\n\n  return [\n    loading,\n    error,\n    data ? pickContractResult(data?.contractBalance) : { balance: '0' },\n    refetch,\n  ];\n}\n","/Users/ssen/Workspace/anchor/app/src/hooks/mantle/use-wallet-balance.ts",["388","389","390"],"import { ApolloError, gql, useQuery } from '@apollo/client';\nimport { MantleRefetch, MantleResponseWithHeight, pickResult } from './types';\nimport { useWallet } from '../use-wallet';\n\ntype ResponseBalance = { Denom: string; Amount: string }[];\nconst queryBalance = gql`\n  query($address: String!) {\n    Balances: BankBalancesAddress(Address: $address) {\n      Height\n      Result {\n        Denom\n        Amount\n      }\n    }\n  }\n`;\n\nexport default function useWalletBalance(): [\n  boolean,\n  ApolloError | undefined,\n  ResponseBalance | null,\n  MantleRefetch,\n] {\n  const wallet = useWallet();\n  const { loading, error, data, refetch } = useQuery<{\n    Balances: MantleResponseWithHeight<ResponseBalance>;\n  }>(queryBalance, {\n    variables: {\n      address: wallet.address,\n    },\n  });\n\n  return [\n    loading,\n    error,\n    data ? pickResult(data.Balances) : null,\n    () => refetch(),\n  ];\n}\n","/Users/ssen/Workspace/anchor/app/src/hooks/mantle/use-basset-balance.ts",[],"/Users/ssen/Workspace/anchor/app/src/components/input.tsx",[],"/Users/ssen/Workspace/anchor/app/src/@anchor-protocol/anchor-js/fabricators/basset-claim.ts",[],"/Users/ssen/Workspace/a