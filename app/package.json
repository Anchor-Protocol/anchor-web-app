    "test": "react-app-rewired test --colors",
    "coverage": "react-app-rewired test --colors --coverage"
  },
  "multiplerun": {
    "start": [
      "PORT=9433 BROWSER=none npx react-app-rewired start",
      "PORT=9433 TS_NODE_PROJECT=tsconfig.script.json node -r ts-node/register -r tsconfig-paths/register scripts/start.ts"
    ],
    "start:chrome": [
      "PORT=9433 BROWSER=none npx react-app-rewired start"
    ]
  },
