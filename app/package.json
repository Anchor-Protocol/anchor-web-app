{
  "name": "@anchor-web-app/app",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "app:dev": "VITE_APP_VERSION=$(git rev-parse --short HEAD) && vite",
    "app:dev:browser": "HTTPS=true PORT=3000 TS_NODE_PROJECT=tsconfig.script.json node -r ts-node/register -r tsconfig-paths/register scripts/start.ts",
    "build": "VITE_APP_VERSION=$(git rev-parse --short HEAD) && tsc --noEmit && vite build",
    "build-storybook": "build-storybook -s public",
    "coverage": "react-scripts test --colors --coverage",
    "start": "multiplerun dev",
    "start:multiple-wallets": "TS_NODE_PROJECT=tsconfig.script.json node -r ts-node/register -r tsconfig-paths/register scripts/start.multiple-wallets.ts",
    "storybook": "start-storybook -h localhost -p 6006 -s public",
    "test": "react-scripts test --colors",
    "test:update": "react-scripts test -u"
  },
  "eslintConfig": {
    "extends": [
      "../.eslintrc.js"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts?(x)",
      "!**/*.d.ts?(x)",
      "!**/*.stories.ts?(x)",
      "!**/__*__/**"
    ]
  },
  "multiplerun": {
    "dev": [
      "yarn run tsc --watch --noEmit",
      [
        "yarn run app:dev",
        {
          "command": "yarn run app:dev:browser",
          "wait": "5000"
        }
      ]
    ]
  },
