{
  "name": "@anchor-web-app/landing",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "start": "multiplerun start",
    "start:chrome": "PORT=9434 react-app-rewired start",
    "app:start": "HTTPS=true SSL_CRT_FILE=$LOCALHOST_HTTPS_CERT SSL_KEY_FILE=$LOCALHOST_HTTPS_KEY PORT=9434 BROWSER=none yarn run react-app-rewired start",
    "app:start:puppeteer": "HTTPS=true PORT=9434 TS_NODE_PROJECT=tsconfig.script.json node -r ts-node/register -r tsconfig-paths/register scripts/start.ts",
    "build": "CI=true react-app-rewired build",
    "test": "react-app-rewired test --colors",
    "coverage": "react-app-rewired test --colors --coverage"
  },
  "multiplerun": {
    "start": [
      "yarn run app:start",
      {
        "command": "yarn run app:start:puppeteer",
        "wait": "20000"
      }
    ]
  },
  "eslintConfig": {
    "extends": [
      "../.eslintrc.js"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts?(x)",
      "!**/*.d.ts?(x)",
      "!**/*.stories.ts?(x)",
      "!**/__*__/**"
    ]
  },
  "browserslist": {
    "production": [
      "chrome > 60",
      "firefox > 60",
      "safari > 12"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@terra-money/terra.js": "^3.0.7",
    "chart.js": "^3.4.1",
    "color": "^3.1.3",
    "d3-scale": "^3.3.0",
    "d3-shape": "^2.1.0",
    "dat.gui": "^0.7.7",
    "date-fns": "^2.22.1",
    "downshift": "^6.1.3",
    "fastdom": "^1.0.10",
    "gl-matrix": "^3.3.0",
    "glslify": "^7.1.1",
    "glslify-loader": "^2.0.0",
    "graphql": "^15.5.1",
    "graphql-tag": "^2.12.5",
    "multiplerun": "^2.0.1",
    "puppeteer": "^10.1.0",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-app-rewire-alias": "^1.0.3",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^17.0.2",
    "react-query": "^3.18.1",
    "react-responsive": "^8.2.0",
    "react-router-dom": "^6.2.1",
    "react-scripts": "4.0.3",
    "react-spinners-kit": "^1.9.1",
    "react-spring": "^8.0.27",
    "regl": "^2.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "resl": "^1.0.3",
    "smooth-scroll": "^16.1.3",
    "stats.js": "^0.17.0",
    "styled-components": "^5.3.0",
    "use-resize-observer": "^7.0.0",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@rocket-scripts/read-package-alias": "^4.0.0-alpha.3",
    "@ssen/jest-transform": "^3.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/autoprefixer": "^10.2.0",
    "@types/color": "^3.0.1",
    "@types/d3-scale": "^3.3.2",
    "@types/d3-shape": "^2.1.3",
    "@types/dat.gui": "^0.7.7",
    "@types/eslint": "^7.2.14",
    "@types/gl-matrix": "^3.2.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.1",
    "@types/postcss-nested": "^4.2.3",
    "@types/ramda": "^0.27.44",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-responsive": "^8.0.3",
    "@types/react-router-dom": "^5.1.8",
    "@types/smooth-scroll": "^16.1.1",
    "@types/stats.js": "^0.17.0",
    "@types/styled-components": "^5.1.11",
    "@types/testing-library__jest-dom": "^5.14.0",
    "@types/testing-library__user-event": "^4.2.0",
    "ts-node": "^10.1.0",
